<launch>
        <node pkg="rtf_sensors" exec="dps310_node" name="altimeter_sphere" output="screen">
                <remap from="altitude" to="dps310_sphere/altitude"/>
                <remap from="temperature" to="dps310_sphere/temperature"/>
		<remap from="pressure" to="dps310_sphere/pressure"/>
		<param name="frame_id" value="dps310" />
		<param name="i2c_address" value="0x77" />
        </node>
</launch>

